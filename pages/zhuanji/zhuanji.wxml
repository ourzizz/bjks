<!--pages/zhuanji/zhuanji.wxml-->

<view class="page">
    <view class="page__hd">
        <view class="page__title">专技考试栏目</view>
    </view>
    <view class="page__bd">
        <view class="weui-grids">
            <block wx:for="{{list.zhuanjilist}}" >
                <navigator url="../zhuanjikaoshi/zhuanjikaoshi?ksid={{item.ksid}}" class="weui-grid" hover-class="weui-grid_active">
                    <image class="weui-grid__icon" src="https://www.alemao.club/weapp/images/{{item.shortname}}.png" />
                    <view class="weui-grid__label">{{item.ksname}}</view>
                </navigator>
            </block>
        </view>
    </view>
</view>

